						</ul>
					</li>
				</ul>
			</nav>
		</div>

		<!-- Main -->
		<section class="wrapper style1">
			<div class="container">
				<form id="form_registro" name="form_registro" style="color: #333">
					<div id="error"> </div>
					Nombres
					<div class="input-group form-group">
						<div class="input-group-prepend">
							<span class="input-group-text"><i class="fas fa-user"></i></span>
						</div>
						<input id="nombre" name="nombre" type="text" class="form-control" placeholder="Nombres">
					</div>
					Apellidos
					<div class="input-group form-group">
						<div class="input-group-prepend">
							<span class="input-group-text"><i class="fas fa-user"></i></span>
						</div>
						<input id="apellido" name="apellido" type="text" class="form-control" placeholder="Apellidos">
					</div>
					Cedula
					<div class="input-group form-group">
						<div class="input-group-prepend">
							<span class="input-group-text"><i class="fas fa-user"></i></span>
						</div>
						<input id="cedula" name="cedula" type="text" class="form-control" placeholder="Cedula">
					</div>
					Ciudad
					<div class="input-group form-group">
						<div class="input-group-prepend">
							<span class="input-group-text"><i class="fas fa-user"></i></span>
						</div>
						<input id="ciudad" name="ciudad" type="text" class="form-control" placeholder="Ciudad">
					</div>
					Domicilio
					<div class="input-group form-group">
						<div class="input-group-prepend">
							<span class="input-group-text"><i class="fas fa-user"></i></span>
						</div>
						<input id="sector" name="sector" type="text" class="form-control" placeholder="Sector">
					</div>
					<div class="input-group form-group">
						<div class="input-group-prepend">
							<span class="input-group-text"><i class="fas fa-user"></i></span>
						</div>
						<input id="calleP" name="calleP" type="text" class="form-control" placeholder="Calle Principal">
						<input id="calleS" name="calleS" type="text" class="form-control" placeholder="Calle Secundaria">
						<input id="referencia" name="descripcion" type="text" class="form-control" placeholder="Referencia">
					</div>
					Enfermedades Padecidas
					<div class="input-group form-group">
						<div class="input-group-prepend">
							<span class="input-group-text"><i class="fas fa-user"></i></span>
						</div>
						<input id="enfermedades" name="enfermedades" type="text" class="form-control"
							placeholder="Enferemedades Padecidas">
					</div>
					Telefono Particular
					<div class="input-group form-group">
						<div class="input-group-prepend">
							<span class="input-group-text"><i class="fas fa-user"></i></span>
						</div>
						<input id="telefono" name="telefono" type="number" class="form-control"
							placeholder="Numero de Telefono">
					</div>
					Telefono de Emergencia
					<div class="input-group form-group">
						<div class="input-group-prepend">
							<span class="input-group-text"><i class="fas fa-user"></i></span>
						</div>
						<input id="telefonoE" name="telefonoE" type="number" class="form-control"
							placeholder="Numero de Telefono">
					</div>
					Correo Electronico
					<div class="input-group form-group">
						<div class="input-group-prepend">
							<span class="input-group-text"><i class="fas fa-user"></i></span>
						</div>
						<input id="correo" name="correo" type="email" class="form-control"
							placeholder="Correo Electronico">
					</div>
					Fecha de Nacimiento
					<div class="input-group form-group">
						<div class="input-group-prepend">
							<span class="input-group-text"><i class="fas fa-user"></i></span>
						</div>
						<input id="fechaNacimiento" name="fechaNacimiento" type="date" class="form-control"
							placeholder="Fecha de Nacimiento">
					</div>
					<!--Extranjero
					<div class="input-group form-group">
						<div class="input-group-prepend">
							<span class="input-group-text"><i class="fas fa-user"></i></span>
						</div>
						<Select id="extranjero" name="extranjero" class="form-control"></select>
					</div>-->

					<div class="form-group">
						<input type="submit" value="Registrar" class="btn float-right login_btn">
					</div>
				</form>
		</section>

